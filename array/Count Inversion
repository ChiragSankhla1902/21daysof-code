












---------------------------------------------------------------time execeding----------------------------------------------------------
long long int inversionCount(long long arr[], long long N)
{
    int i=0,count=0,j;
    while(i<=N-1){
        for(j=i;j<N;j++){
            if(arr[i]>arr[j]){
                count++;
            }
        }
        i++;
    }
    return(count);

}
