int minJumps(int arr[], int n){
    int i,value=0,count=0;
    for(i=0;i<n-1;i+=value){
        if(arr[i]==0){
            break;
        }
        else if(i==n-1||arr[i]>=n-i){
            count+=1;
            break;
        }
        else{
            value=arr[i];
            count+=1;
        }
    }
    return count;
}
